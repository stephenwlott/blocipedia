<tr id="wiki-<%= wiki.id %>">
  <td><h4><%= wiki.title %><h4></td>
  <td><h4><%= wiki.user.try(:name) %><h4></td>
  <% if wiki.private %>
    <td><h4><center>private</center><h4></td>
  <% else %>
    <td><h4><center>public</center><h4></td>
  <% end %>
  <td><h4><%= link_to "View", user_wiki_path(wiki.user_id, wiki.id) %></h4></td>
  <td><h4><%= link_to "Edit", edit_user_wiki_path(wiki.user_id, wiki.id) %></h4></td>
  <% if current_user %>
    <% if current_user.is_admin? %>
      <td><h4><%= link_to "Delete", user_wiki_path(wiki.user_id, wiki.id), method: :delete, remote: true, data: { confirm: 'Are you sure?' } %></h4></td>
    <% end %>
  <% end %>
</tr>